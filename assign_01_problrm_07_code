package prob7;
import java.util.Scanner;
public class Prob7 {

    public static void main(String[] args) {
         Scanner input= new Scanner(System.in);
        int a,b,c,d,X;
        
        System.out.println("enter 4 numbers");
        a= input.nextInt();
b= input.nextInt();
c= input.nextInt();
d= input.nextInt();
   X=(a*b)-(c*d);
   System.out.println("Difference ="+X);
  
    }
    
}
