package prob6;
import java.util.Scanner;
public class Prob6 {
    public static void main(String[] args) {
        Scanner input= new Scanner(System.in);
 
         int x;
         System.out.println("enter a number");
         x= input.nextInt();
  if (x % 2 ==0)
  System.out.println("even");
  else
   System.out.println("odd");
    }
    
}
